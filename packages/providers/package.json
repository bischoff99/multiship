{
  "name": "@pkg/providers",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "dependencies": {
    "@easypost/api": "^8.2.0",
    "ioredis": "^5.8.0",
    "shippo": "^2.15.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:fix": "jest --updateSnapshot --verbose",
    "test:debug": "jest --verbose --detectOpenHandles --runInBand",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:performance": "jest --testPathPatterns=performance",
    "clean": "rm -rf dist coverage .jest-cache",
    "typecheck": "tsc --noEmit",
    "verify:veeqo": "tsx src/test/verify-veeqo-integration.ts",
    "verify:veeqo:health": "tsx src/test/verify-veeqo-integration.ts --health-only",
    "verify:veeqo:config": "tsx src/test/verify-veeqo-integration.ts --config-only"
  }
}