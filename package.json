{
  "name": "multiship",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "start": "pnpm --filter @app/api start",
    "db:push": "pnpm --filter @pkg/db db:push",
    "db:studio": "pnpm --filter @pkg/db prisma studio"
  }
}
